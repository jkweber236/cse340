<% if (title) { %>
   <h1><%= title %></h1>
   <% } else {
      res.redirect('/')
} %>

<%- messages() %>

<% if (errors) { %>
   <ul class="notice">
   <% errors.array().forEach(error => { %>
      <% if (error.msg !== "Invalid value") { %>
         <li><%= error.msg %></li>
      <% } %>
   <% }) %>
   </ul>
<% } %>

<ul class="management-links">
   <li><a href="/inv/add-classification">Add New Classification</a></li>
   <li><a href="/inv/add-inventory">Add New Inventory</a></li>
</ul>